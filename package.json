{
  "name": "website-cdk",
  "version": "0.1.0",
  "bin": {
    "website-cdk": "bin/website-cdk.js"
  },
  "scripts": {
    "build": "npm install && tsc",
    "clean": "rm -rf dist build node_modules package-lock.json",
    "release": "npm run clean && npm run build && cdk deploy --all",
    "format": "eslint \"**/*.js\" \"**/*.ts\" --fix",
    "watch": "tsc -w",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.134",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "aws-cdk": "2.130.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.130.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
